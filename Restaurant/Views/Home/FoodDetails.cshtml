@model Restaurant.Models.Food

@{
    ViewBag.Title = "FoodDetails";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="popular" id="popular">
    <h2>FoodDetails</h2>
    <p>@Model.FoodName</p>
    <p>@Model.CateId</p>
    <p>@Model.Description</p>
    <p>@Model.Price</p>
    <p><img width="250" height="250" src="@("/Content/images/"+Model.Image)" asp-append-version="true" /></p>
</section>